{
    "name": "@softvisio/cli",
    "version": "3.85.6",
    "description": "Softisio CLI Tool",
    "keywords": [
        "softvisio",
        "cli"
    ],
    "homepage": "https://softvisio-node.github.io/cli/",
    "bugs": {
        "url": "https://github.com/softvisio-node/cli/issues",
        "email": "root <root@softvisio.net>"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/softvisio-node/cli.git"
    },
    "license": "ISC",
    "author": "root <root@softvisio.net>",
    "type": "module",
    "imports": {
        "#core/*": "@softvisio/core/*",
        "#lib/*": "./lib/*.js",
        "#resources/*": "./resources/*"
    },
    "exports": {
        "./*": "./lib/*.js",
        "./resources/*": "./resources/*"
    },
    "bin": {
        "softvisio-cli": "bin/softvisio-cli.js"
    },
    "files": [
        "npm-shrinkwrap.json",
        "/bin",
        "/lib",
        "/resources",
        "/src"
    ],
    "scripts": {
        "install": "node bin/install.js",
        "test": "node --test tests/**/*.test.js"
    },
    "dependencies": {
        "@prettier/plugin-oxc": "^0.0.4",
        "@prettier/plugin-xml": "^3.4.1",
        "@softvisio/core": "^7.175.0",
        "@softvisio/eslint-plugin": "^1.0.1",
        "@stylistic/eslint-plugin": "^5.0.0",
        "@vue/compiler-sfc": "^3.2.36",
        "cssnano": "^7.0.0",
        "editorconfig": "^3.0.0",
        "eslint": "^9.0.0",
        "eslint-plugin-eslint-comments": "^3.2.0",
        "eslint-plugin-import": "^2.31.0",
        "eslint-plugin-simple-import-sort": "^12.1.1",
        "eslint-plugin-unicorn": "^60.0.0",
        "eslint-plugin-vue": "^10.0.0",
        "espree": "^10.0.1",
        "estraverse": "^5.3.0",
        "html-minifier-terser": "^7.2.0",
        "postcss": "^8.2.4",
        "prettier": "^3.0.0",
        "prettier-plugin-packagejson": "^2.2.9",
        "prettier-plugin-sh": "^0.18.0",
        "sharp": "^0.34.0",
        "terser": "^5.5.1",
        "typescript-eslint": "^8.0.0",
        "vue-eslint-parser": "^10.0.0"
    }
}
